<% if (errors.length > 0) { %>
<div class="alert alert-warning">
  <p>
    <b
      >Há problemas com o formulário enviado! Corrija-os e tente enviar
      novamente.</b
    >
  </p>
  <ul class="m-0">
    <% errors.forEach(error => { %>
    <li><%= error.msg %></li>
    <% }) %>
  </ul>
</div>
<% } %>
